{
  "name": "invaders",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/JustinLloyd/woad-wash-worldgit .git"
  },
  "scripts": {
    "start": "parcel serve ./game-server/src/html/www.html",
    "build": "parcel build ./game-server/src/html/www.html --public-url=./ --no-content-hash --dist-dir final --no-optimize --detailed-report",
    "watch": "parcel watch ./src/html/wwww.html",
    "prebuild": "rimraf ./final"
  },
  "file": [
    "game-client/src/scripts/",
    "game-client/src/css/",
    "game-client/src/html/**/*",
    "game-client/src/assets/**/*",
    "game-server/src/scripts/",
    "build/",
    "dist/"
  ],
  "dependencies": {
    "systemjs": "^6.10.2",
    "typescript": "^4.3.5",
    "@pixi/canvas-renderer": "^6.1.1",
    "@types/gsap": "^3.0.0",
    "@types/jquery": "^3.5.6",
    "eventemitter3": "^4.0.7",
    "gsap": "^3.7.1",
    "inline-scripts": "^1.7.4",
    "jquery": "^3.6.0",
    "pixi-filters": "^4.1.4",
    "pixi.js": "^6.0.4"
  },
  "devDependencies": {
    "parcel": "next",
    "rimraf": "^3.0.2",
    "sharp": "^0.28.3"
  }
}
